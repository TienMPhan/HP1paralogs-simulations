group     rigid1 id  19:67
group     rigid2 id  210:258
group     rigid3 id  112:169 303:360
group     rigid union rigid1 rigid2 rigid3
group     nonrigid subtract all rigid
#---------------------------------------------------------

special_bonds lj/coul 0.0 1.0 1.0

neighbor      3.5 multi
neigh_modify  exclude molecule/intra rigid1
neigh_modify  exclude molecule/intra rigid2
neigh_modify  exclude molecule/intra rigid3
neigh_modify  every 10 delay 0

### Time step
reset_timestep 0
timestep    ${dt}

### Ensemble / Thermostat
velocity    all create $t ${seedV}
fix         1 all langevin $t $t ${damp} ${seedT}
fix         2 rigid1 rigid/nve molecule
fix         3 rigid2 rigid/nve molecule
fix         5 rigid3 rigid/nve single
fix         4 nonrigid nve molecule

### Outputting useful information in the log file
thermo     ${thermointrv}
thermo_style custom step evdwl ecoul ebond temp

# Saving the simulation snapshots in a trajectory file
dump       VMD1 all xtc ${dumpintrv} hp1a.xtc

# Specifying the runlength
run        ${runtime}
#temper     ${runtime} ${temperintrv} $t 1 841384 1874

# Restart file
write_data restart.data nocoeff
